{
    "name": "yaapi/di",
    "type": "library",
    "description": "YaAPI DI Package",
    "keywords": ["yaapi", "dependency injection", "di", "ioc", "container"],
    "homepage": "https://github.com/yaapi/di",
    "license": "LGPL-2.1+",
    "authors": [
        {
            "name": "YaAPI DI Package Contributors",
            "homepage": "https://github.com/yaapi/di/graphs/contributors"
        }
    ],
    "require": {
        "php": ">=5.4.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7.7",
        "yaapi/coding-standards": "1.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Yaapi\\DI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yaapi\\DI\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}